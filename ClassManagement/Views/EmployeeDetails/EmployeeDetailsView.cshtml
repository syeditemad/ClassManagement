@model ClassManagement_ModelLibrary.Class_Model.EmployeePersonelDetails

@{
    ViewData["Title"] = "Employee Details";
    Layout = null;
}
<div>
    <h4 class="text-uppercase text-center">EmployeeDetails</h4>
    <hr />

    <div class="row">
        <div class=" col-md-12">
            <div class="row">
                <div class="form-group col-md-6">
                    <label asp-for="FirstName" class="control-label"></label>
                    <input asp-for="FirstName" class="form-control" readonly />
                </div>
                <div class="form-group col-md-6">
                    <label asp-for="SurName" class="control-label"></label>
                    <input asp-for="SurName" class="form-control" readonly />
                </div>
                <div class="form-group col-md-6">
                    <label asp-for="MiddleName" class="control-label"></label>
                    <input asp-for="MiddleName" class="form-control" readonly />
                </div>
                <div class="form-group col-md-6">
                    <label asp-for="Gender" class="control-label"></label>
                    <input asp-for="Gender" class="form-control" readonly />
                </div>
                <div class="form-group col-md-6">
                    <label asp-for="DateOfBirth" class="control-label"></label>
                    <input asp-for="DateOfBirth" class="form-control" readonly />
                </div>
                <div class="form-group col-md-6">
                    <label asp-for="DepartMent" class="control-label"></label>
                    <input asp-for="DepartMent" class="form-control" readonly />
                </div>
                <div class="form-group col-md-6">
                    <label asp-for="Email" class="control-label"></label>
                    <input asp-for="Email" class="form-control" readonly />
                </div>
                <div class="form-group col-md-6">
                    <label asp-for="PhoneNumeber" class="control-label"></label>
                    <input asp-for="PhoneNumeber" class="form-control" readonly />
                    <span asp-validation-for="PhoneNumeber" class="text-danger"></span>
                </div>
                <div class="form-group col-md-6">
                    <label asp-for="Landmark" class="control-label"></label>
                    <input asp-for="Landmark" class="form-control" readonly />
                    <span asp-validation-for="Landmark" class="text-danger"></span>
                </div>
                <div class="form-group col-md-6">
                    <label asp-for="Countryid" class="control-label">Country</label>
                    <select asp-for="Countryid"
                            class="form-control text-center"
                            asp-items="ViewBag.CountryList" disabled>
                    </select>
                    <span asp-validation-for="Countryid" class="text-danger"></span>
                </div>
            </div>
               

            </div>
        </div>
    </div>
<div>
    @*<a asp-action="Edit" class="btn btn-info btn-block disabled" asp-route-id="@Model.EmployeeId">Edit</a> |*@
    @*<a asp-action="Index">Back to List</a>*@
</div>

